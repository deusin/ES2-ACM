<?xml version="1.0" encoding="utf-8" ?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:noNamespaceSchemaLocation="../Schemas/Amplitude.Unity.Gui.GuiElement.xsd">
  <TableGuiElement Name ="EndGameSummaryTable" Height="54">
    <Column Name="ScoreVictoryStatus" Description="ScoreDescription" Width="120" Prefab="Prefabs/Gui/Table/TableCellScoreRank" />

    <Column Name="PlayerEmpire" Description="FactionDescription" FilterPropertyName="PlayerFactionName" Width="210" Prefab="Prefabs/Gui/Table/TableCellOfflineEmpire">
      <Filter>%FactionCraversTitle</Filter>
      <Filter>%FactionSophonsTitle</Filter>
      <Filter>%FactionVenetiansTitle</Filter>
      <Filter>%FactionTerransTitle</Filter>
      <Filter>%FactionMezariTitle</Filter>
      <Filter>%FactionSheredynTitle</Filter>
      <Filter>%FactionHoratioTitle</Filter>
      <Filter>%FactionVampirilisTitle</Filter>
      <Filter>%FactionUnfallenTitle</Filter>
      <Filter>%FactionTimeLordsTitle</Filter>
      <Filter>%FactionVaultersTitle</Filter>
      <Filter>%FactionMajorHisshosTitle</Filter>
      <Filter>%FactionUmbralChoirTitle</Filter>
      <Filter>%FactionTemplarsTitle</Filter>
      <Filter>%FactionCustomTitle</Filter>
    </Column>

    <Column Name="HasWon" Description="WinnerEmpiresDescription" FilterPropertyName="AchievedVictoryTitle" Width="210" Prefab="Prefabs/Gui/Table/TableCellGameResults">
      <Filter>%VictoryScoreTitle</Filter>
      <Filter>%VictorySupremacyTitle</Filter>
      <Filter>%VictoryConquestTitle</Filter>
      <Filter>%VictoryScienceTitle</Filter>
      <Filter>%VictoryEconomicTitle</Filter>
      <Filter>%VictoryWonderTitle</Filter>
      <Filter>%VictoryMilitaryTitle</Filter>
    </Column>

    <Column Name="TurnPlusOne" Width="60" Prefab="Prefabs/Gui/Table/TableCellText" />

    <Column Name="GameDifficulty" Description="GameDifficultyDescription" Width="100" FilterMenuWidth="140" Prefab="Prefabs/Gui/Table/TableCellText">
      <Filter>%SettingGameDifficultyNewbieTitle</Filter>
      <Filter>%SettingGameDifficultyEasyTitle</Filter>
      <Filter>%SettingGameDifficultyNormalTitle</Filter>
      <Filter>%SettingGameDifficultyHardTitle</Filter>
      <Filter>%SettingGameDifficultySeriousTitle</Filter>
      <Filter>%SettingGameDifficultyImpossibleTitle</Filter>
      <Filter>%SettingGameDifficultyEndlessTitle</Filter>
    </Column>

    <Column Name="GameSpeed" Description="GameSpeedDescription" Width="100" FilterMenuWidth="140" Prefab="Prefabs/Gui/Table/TableCellText">
      <Filter>%SettingGameSpeedFastTitle</Filter>
      <Filter>%SettingGameSpeedNormalTitle</Filter>
      <Filter>%SettingGameSpeedSlowTitle</Filter>
      <Filter>%SettingGameSpeedEndlessTitle</Filter>
    </Column>

    <Column Name="SessionMode" Width="100" FilterMenuWidth="140" Prefab="Prefabs/Gui/Table/TableCellSessionInfo">
      <Filter>%SessionModeSingle</Filter>
      <!--<Filter>%SessionModePrivate</Filter>-->
      <Filter>%SessionModeProtected</Filter>
      <Filter>%SessionModePublic</Filter>
    </Column>

    <Column Name="Date" Width="140" Prefab="Prefabs/Gui/Table/TableCellText" />
    <Column Name="EndGameSummary" Width="80" Prefab="Prefabs/Gui/Table/TableCellScoreScreenButton" DisableSorting="true" />
  </TableGuiElement>
</Datatable>