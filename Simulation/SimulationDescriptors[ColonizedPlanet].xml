<?xml version="1.0" encoding="utf-8" ?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:noNamespaceSchemaLocation="../Schemas/Amplitude.Unity.Simulation.SimulationDescriptor.xsd">
  <SimulationDescriptor Name="ClassColonizedPlanet" Type="ClassColonizedPlanet">
    <!-- ESG -->
    <Property Name="PlanetFoodDynamic"                  BaseValue="0" MinValue="Negative"/>
    <Property Name="PlanetIndustryDynamic"              BaseValue="0" MinValue="Negative"/>
    <Property Name="PlanetDustDynamic"                  BaseValue="0" MinValue="Negative"/>
    <Property Name="PlanetScienceDynamic"               BaseValue="0" MinValue="Negative"/>
    <Property Name="PlanetPrestigeDynamic"              BaseValue="0" MinValue="Negative"/>

    <!-- ########################################################## -->
    <!-- ######################## PROPERTY ######################## -->
    <!-- ########################################################## -->
    <Property Name="OwnedPlanets"                           BaseValue="1" />


    <Property Name="PlanetPopulationFood"      MinValue="Negative" />
    <Property Name="PlanetPopulationIndustry"  MinValue="Negative" />
    <Property Name="PlanetPopulationDust"      MinValue="Negative" />
    <Property Name="PlanetPopulationScience"   MinValue="Negative" />
    <Property Name="PlanetPopulationPrestige"  MinValue="Negative" />

    <Property Name="PlanetDepletingPopulationFood"      MinValue="Negative" />
    <Property Name="PlanetDepletingPopulationIndustry"  MinValue="Negative" />
    <Property Name="PlanetDepletingPopulationDust"      MinValue="Negative" />
    <Property Name="PlanetDepletingPopulationScience"   MinValue="Negative" />
    <Property Name="PlanetDepletingPopulationPrestige"  MinValue="Negative" />

    <Property Name="PlanetParasitedPopulationFood"      MinValue="Negative" />
    <Property Name="PlanetParasitedPopulationIndustry"  MinValue="Negative" />
    <Property Name="PlanetParasitedPopulationDust"      MinValue="Negative" />
    <Property Name="PlanetParasitedPopulationScience"   MinValue="Negative" />
    <Property Name="PlanetParasitedPopulationPrestige"  MinValue="Negative" />

    <Property Name="DepletingPopulation"            BaseValue="0"  MinValue="Negative" />
    <Property Name="DepletingPopulationBoolean"     BaseValue="0"  MaxValue="1" />
    <Property Name="DepletingAndParasitedBonusFIDS" BaseValue="0"  MinValue="Negative" />

    <Property Name="DefaultPlanetFood"/>
    <Property Name="DefaultPlanetIndustry"/>
    <Property Name="DefaultPlanetDust"/>
    <Property Name="DefaultPlanetScience"/>
    <Property Name="DefaultPlanetPrestige"/>

    <Property Name="PlanetInitialFoodFromMiningProbes"/>
    <Property Name="PlanetInitialIndustryFromMiningProbes"/>
    <Property Name="PlanetInitialDustFromMiningProbes"/>
    <Property Name="PlanetInitialScienceFromMiningProbes"/>
    <Property Name="PlanetHasBonusesFromMiningProbes"/>

    <Property Name="PlanetHeroFIDSIPercent"/>
    <Property Name="PlanetHeroFIDSIFlat"/>

    <Property Name="PlanetFIDSI" MinValue="Negative"/>
    <Property Name="PlanetFIDSIDevelopment"/>
    <Property Name="PlanetFIDS" MinValue="Negative"/>

    <Property Name="PlanetFood" MinValue="Negative"/>
    <Property Name="PlanetIndustry" MinValue="Negative"/>
    <Property Name="PlanetDust" MinValue="Negative"/>
    <Property Name="PlanetScience" MinValue="Negative"/>
    <Property Name="PlanetPrestige" MinValue="Negative"/>

    <!-- FIDSI generated by the Assigned Hero (feedback only) -->
    <Property Name="BonusHeroPlanetFood"        BaseValue="0"/>
    <Property Name="BonusHeroPlanetIndustry"    BaseValue="0"/>
    <Property Name="BonusHeroPlanetDust"        BaseValue="0"/>
    <Property Name="BonusHeroPlanetScience"     BaseValue="0"/>
    <Property Name="BonusHeroPlanetPrestige"    BaseValue="0"/>

    <Property Name="PlanetOutpostFood" MinValue="Negative"/>
    <Property Name="PlanetOutpostIndustry" MinValue="Negative"/>
    <Property Name="PlanetOutpostDust" MinValue="Negative"/>
    <Property Name="PlanetOutpostScience" MinValue="Negative"/>
    <Property Name="PlanetOutpostPrestige" MinValue="Negative"/>


    <Property Name="OverPopulationThreshold"                BaseValue="0" />
    <Property Name="PopulationPercentForOverPopulation"     BaseValue="0.7" />
    <Property Name="OverPopulationMalus"                    BaseValue="0"  MinValue="Negative" MaxValue="0"/>
    <Property Name="ParasitedPopulation"                    BaseValue="0"   />
    <Property Name="MaximumPopulationFromMothership"/>
    <Property Name="ShouldOverrideBaseMaximumPopulation" BaseValue="0" />

    <Property Name="PopulationCountPulsos" BaseValue="0" />
    <Property Name="PopulationCountRemnant" BaseValue="0" />
    <Property Name="PopulationCountSefaloros" BaseValue="0" />
    <Property Name="PopulationCountGalvrans" BaseValue="0" />
    <Property Name="PopulationCountHarmony" BaseValue="0" />

    <Property Name="PotentialPlanetRawPrestige" BaseValue="0" />
    <Property Name="PotentialPlanetRawScience" BaseValue="0" />
    <Property Name="PotentialPlanetRawFood" BaseValue="0" />
    <Property Name="PotentialPlanetRawHappiness" BaseValue="0" />
    <Property Name="PotentialPlanetRawHappinessIfDeposit" BaseValue="0"/>
    <Property Name="SuperPopulationCount" BaseValue="0"/>

    <Property Name="TemplePopulationCount" BaseValue="0"/>
    <Property Name="HasTemple" BaseValue="0"/>

    <!-- Utopian Architecture -->
		<Property Name="UtopianUnhappyReduction"					BaseValue="0" MinValue="Negative"/>
    <Property Name="UtopianUnhappyReductionPerPop"		BaseValue="0" MinValue="Negative"/>

    <!-- 2023 Patch -->
    <Property Name="PlanetFoodModifier"      BaseValue="1"/>
    <Property Name="PlanetIndustryModifier"  BaseValue="1"/>
    <Property Name="PlanetDustModifier"      BaseValue="1"/>
    <Property Name="PlanetScienceModifier"   BaseValue="1"/>
    <Property Name="PlanetPrestigeModifier"  BaseValue="1"/>
    <Property Name="PlanetInitialFIDSBonus" BaseValue="0" MinValue="Negative"/>
    <Property Name="PlanetInitialFIDSBonusPerPopulation" MinValue="Negative"/>
    <Property Name="PlanetInitialFIDSBonusPerPopulationFood" MinValue="Negative"/>
    <Property Name="PlanetInitialFIDSBonusPerPopulationIndustry" MinValue="Negative"/>
    <Property Name="PlanetInitialFIDSBonusPerPopulationDust" MinValue="Negative"/>
    <Property Name="PlanetInitialFIDSBonusPerPopulationScience" MinValue="Negative"/>
    <Property Name="HappinessMalusReduction"					BaseValue="0" MinValue="Negative"/>
    <Property Name="HappinessMalusReductionPerPop"		BaseValue="0" MinValue="Negative"/>
    <Property Name="PlanetIndustryFromSenatorSkill"/>
    <Property Name="PlanetFIDSIFromSenatorSkill"/>
    <Property Name="PlanetFIDSFromMothership"/>

    <Property Name="PlanetFoodFromDevelopment"/>
    <Property Name="PlanetIndustryFromDevelopment"/>
    <Property Name="PlanetDustFromDevelopment"/>
    <Property Name="PlanetScienceFromDevelopment"/>
    <Property Name="PlanetPrestigeFromDevelopment"/>
    <Property Name="PlanetFIDSFromDevelopment"/>
    <Property Name="PlanetFIDSIFromDevelopment"/>

    <Property Name="FoodFromPopulation" MinValue="Negative"/>
    <Property Name="IndustryFromPopulation" MinValue="Negative"/>
    <Property Name="DustFromPopulation" MinValue="Negative"/>
    <Property Name="ScienceFromPopulation" MinValue="Negative"/>
    <Property Name="PrestigeFromPopulation" MinValue="Negative"/>

    <!-- ########################################################## -->
    <!-- ######################## MODIFIER ######################## -->
    <!-- ########################################################## -->

    <!-- Utopian Architecture and Divine Indifference -->
    <BinaryModifier TargetProperty="Happiness" Operation="Addition" Left="$(UtopianUnhappyReductionPerPop)" BinaryOperation="Multiplication" Right="$(Population)" Path="ClassColonizedPlanet" Priority="10" TooltipHidden="true"/>

    <BinaryModifier TargetProperty="Happiness" Operation="Addition" Left="$(HappinessMalusReductionPerPop)" BinaryOperation="Multiplication" Right="$(Population)"  Priority="10" TooltipHidden="true"/>
    <BinaryModifier TargetProperty="HappinessMalusReductionPerPop" Operation="Addition" Left="$(HappinessMalusReduction)" BinaryOperation="Multiplication" Right="$(HappinessRaw)" Path="ClassColonizedPlanet" Priority="10" TooltipHidden="true"/>

    <BinaryModifier TargetProperty="PlanetFood"        Operation="Addition"    Left="$(PlanetInitialFood)"     BinaryOperation="Multiplication" Right="$(Population)" Priority="-5"/>
    <BinaryModifier TargetProperty="PlanetIndustry"    Operation="Addition"    Left="$(PlanetInitialIndustry)" BinaryOperation="Multiplication" Right="$(Population)" Priority="-5"/>
    <BinaryModifier TargetProperty="PlanetDust"        Operation="Addition"    Left="$(PlanetInitialDust)"     BinaryOperation="Multiplication" Right="$(Population)" Priority="-5"/>
    <BinaryModifier TargetProperty="PlanetScience"     Operation="Addition"    Left="$(PlanetInitialScience)"  BinaryOperation="Multiplication" Right="$(Population)" Priority="-5"/>
    <BinaryModifier TargetProperty="PlanetPrestige"    Operation="Addition"    Left="$(PlanetInitialPrestige)" BinaryOperation="Multiplication" Right="$(Population)" Priority="-5"/>

    <Modifier TargetProperty="RawPopulationFood"         Operation="Addition" Value="$(PlanetInitialFood)"        Path="ClassColonizedPlanet/ClassPopulationPlanet" TooltipHidden="true"/>
    <Modifier TargetProperty="RawPopulationIndustry"     Operation="Addition" Value="$(PlanetInitialIndustry)"    Path="ClassColonizedPlanet/ClassPopulationPlanet" TooltipHidden="true"/>
    <Modifier TargetProperty="RawPopulationDust"         Operation="Addition" Value="$(PlanetInitialDust)"        Path="ClassColonizedPlanet/ClassPopulationPlanet" TooltipHidden="true"/>
    <Modifier TargetProperty="RawPopulationScience"      Operation="Addition" Value="$(PlanetInitialScience)"     Path="ClassColonizedPlanet/ClassPopulationPlanet" TooltipHidden="true"/>
    <Modifier TargetProperty="RawPopulationPrestige"     Operation="Addition" Value="$(PlanetInitialPrestige)"    Path="ClassColonizedPlanet/ClassPopulationPlanet" TooltipHidden="true"/>

    <Modifier TargetProperty="PlanetFood"             Operation="Addition" Value="$(PlanetFIDSI)" />
    <Modifier TargetProperty="PlanetIndustry"         Operation="Addition" Value="$(PlanetFIDSI)" />
    <Modifier TargetProperty="PlanetDust"             Operation="Addition" Value="$(PlanetFIDSI)" />
    <Modifier TargetProperty="PlanetScience"          Operation="Addition" Value="$(PlanetFIDSI)" />
    <Modifier TargetProperty="PlanetPrestige"         Operation="Addition" Value="$(PlanetFIDSI)" />

    <Modifier TargetProperty="PlanetFood"             Operation="Addition" Value="$(PlanetFIDS)"  />
    <Modifier TargetProperty="PlanetIndustry"         Operation="Addition" Value="$(PlanetFIDS)"  />
    <Modifier TargetProperty="PlanetDust"             Operation="Addition" Value="$(PlanetFIDS)"  />
    <Modifier TargetProperty="PlanetScience"          Operation="Addition" Value="$(PlanetFIDS)"  />

    <Modifier TargetProperty="PlanetFood"             Operation="Addition" Value="$(DefaultPlanetFood)"     />
    <Modifier TargetProperty="PlanetIndustry"         Operation="Addition" Value="$(DefaultPlanetIndustry)" />
    <Modifier TargetProperty="PlanetDust"             Operation="Addition" Value="$(DefaultPlanetDust)"     />
    <Modifier TargetProperty="PlanetScience"          Operation="Addition" Value="$(DefaultPlanetScience)"  />
    <Modifier TargetProperty="PlanetPrestige"         Operation="Addition" Value="$(DefaultPlanetPrestige)" />

    <Modifier TargetProperty="PlanetFood"             Operation="Addition" Value="$(FoodFromPopulation)"     />
    <Modifier TargetProperty="PlanetIndustry"         Operation="Addition" Value="$(IndustryFromPopulation)" />
    <Modifier TargetProperty="PlanetDust"             Operation="Addition" Value="$(DustFromPopulation)"     />
    <Modifier TargetProperty="PlanetScience"          Operation="Addition" Value="$(ScienceFromPopulation)"  />
    <Modifier TargetProperty="PlanetPrestige"         Operation="Addition" Value="$(PrestigeFromPopulation)" />

    <!-- Craver Depletion and Enslaved Populations -->
    <BinaryModifier TargetProperty="DepletingAndParasitedBonusFIDS"       Operation="Addition" Left="$(DepletingPopulation)"     BinaryOperation="Multiplication" Right="1.5" />
    <Modifier TargetProperty="DepletingAndParasitedBonusFIDS"             Operation="Addition" Value="$(ParasitedPopulation)"   />
    <Modifier TargetProperty="DepletingAndParasitedBonusFIDS"             Operation="Division" Value="$(Population)"   />

    <Modifier TargetProperty="PlanetFood"                       Operation="Percent"     Value="$(DepletingAndParasitedBonusFIDS)" />
    <Modifier TargetProperty="PlanetIndustry"                   Operation="Percent"     Value="$(DepletingAndParasitedBonusFIDS)" />
    <Modifier TargetProperty="PlanetDust"                       Operation="Percent"     Value="$(DepletingAndParasitedBonusFIDS)" />
    <Modifier TargetProperty="PlanetScience"                    Operation="Percent"     Value="$(DepletingAndParasitedBonusFIDS)" />
    <Modifier TargetProperty="PlanetPrestige"                   Operation="Percent"     Value="$(DepletingAndParasitedBonusFIDS)" />

    <Modifier TargetProperty="DepletingPopulationBoolean"       Operation="Addition"        Value="$(DepletingPopulation)" />
    <BinaryModifier TargetProperty="PlanetDepletionFIDSPenalty" Operation="Multiplication"  Left="1"     BinaryOperation="Subtraction"   Right="$(DepletingPopulationBoolean)" />
    
    <!-- Craver Depletion and Enslaved Populations -->
    <Modifier TargetProperty="PlanetFood"                       Operation="Percent"     Value="$(PlanetDepletionFIDSPenaltyGUI)" />
    <Modifier TargetProperty="PlanetIndustry"                   Operation="Percent"     Value="$(PlanetDepletionFIDSPenaltyGUI)" />
    <Modifier TargetProperty="PlanetDust"                       Operation="Percent"     Value="$(PlanetDepletionFIDSPenaltyGUI)" />
    <Modifier TargetProperty="PlanetScience"                    Operation="Percent"     Value="$(PlanetDepletionFIDSPenaltyGUI)" />
    <Modifier TargetProperty="PlanetPrestige"                   Operation="Percent"     Value="$(PlanetDepletionFIDSPenaltyGUI)" />

    <Modifier TargetProperty="PlanetFood"             Operation="Percent" Value="$(PlanetHeroFIDSIPercent)" />
    <Modifier TargetProperty="PlanetIndustry"         Operation="Percent" Value="$(PlanetHeroFIDSIPercent)" />
    <Modifier TargetProperty="PlanetDust"             Operation="Percent" Value="$(PlanetHeroFIDSIPercent)" />
    <Modifier TargetProperty="PlanetScience"          Operation="Percent" Value="$(PlanetHeroFIDSIPercent)" />
    <Modifier TargetProperty="PlanetPrestige"         Operation="Percent" Value="$(PlanetHeroFIDSIPercent)" />

    <Modifier TargetProperty="PlanetFood"             Operation="Addition" Value="$(PlanetHeroFIDSIFlat)"   />
    <Modifier TargetProperty="PlanetIndustry"         Operation="Addition" Value="$(PlanetHeroFIDSIFlat)"   />
    <Modifier TargetProperty="PlanetDust"             Operation="Addition" Value="$(PlanetHeroFIDSIFlat)"   />
    <Modifier TargetProperty="PlanetScience"          Operation="Addition" Value="$(PlanetHeroFIDSIFlat)"   />
    <Modifier TargetProperty="PlanetPrestige"         Operation="Addition" Value="$(PlanetHeroFIDSIFlat)"   />

    <BinaryModifier TargetProperty="PlanetFood"       Operation="Addition" Left="$(PlanetInitialFoodFromMiningProbes)"     BinaryOperation="Multiplication" Right="$(Population)" />
    <BinaryModifier TargetProperty="PlanetIndustry"   Operation="Addition" Left="$(PlanetInitialIndustryFromMiningProbes)" BinaryOperation="Multiplication" Right="$(Population)" />
    <BinaryModifier TargetProperty="PlanetDust"       Operation="Addition" Left="$(PlanetInitialDustFromMiningProbes)"     BinaryOperation="Multiplication" Right="$(Population)" />
    <BinaryModifier TargetProperty="PlanetScience"    Operation="Addition" Left="$(PlanetInitialScienceFromMiningProbes)"  BinaryOperation="Multiplication" Right="$(Population)" />

    <Modifier TargetProperty="PlanetHasBonusesFromMiningProbes" Operation="Addition" Value="$(PlanetInitialFoodFromMiningProbes)"       TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetHasBonusesFromMiningProbes" Operation="Addition" Value="$(PlanetInitialIndustryFromMiningProbes)"   TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetHasBonusesFromMiningProbes" Operation="Addition" Value="$(PlanetInitialDustFromMiningProbes)"       TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetHasBonusesFromMiningProbes" Operation="Addition" Value="$(PlanetInitialScienceFromMiningProbes)"    TooltipHidden="true"/>

    <Modifier TargetProperty="PlanetFoodReceiver"             Operation="Addition" Value="$(PlanetFood)"     Path="./ClassColonizedStarSystem" TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetIndustryReceiver"         Operation="Addition" Value="$(PlanetIndustry)" Path="./ClassColonizedStarSystem" TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetDustReceiver"             Operation="Addition" Value="$(PlanetDust)"     Path="./ClassColonizedStarSystem" TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetScienceReceiver"          Operation="Addition" Value="$(PlanetScience)"  Path="./ClassColonizedStarSystem" TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetPrestigeReceiver"         Operation="Addition" Value="$(PlanetPrestige)" Path="./ClassColonizedStarSystem" TooltipHidden="true"/>

    <Modifier TargetProperty="BonusHeroSystemFood"      Operation="Addition" Value="$(BonusHeroPlanetFood)"     Path="./ClassColonizedStarSystem" TooltipHidden="true"/>
    <Modifier TargetProperty="BonusHeroSystemIndustry"  Operation="Addition" Value="$(BonusHeroPlanetIndustry)" Path="./ClassColonizedStarSystem" TooltipHidden="true"/>
    <Modifier TargetProperty="BonusHeroSystemDust"      Operation="Addition" Value="$(BonusHeroPlanetDust)"     Path="./ClassColonizedStarSystem" TooltipHidden="true"/>
    <Modifier TargetProperty="BonusHeroSystemScience"   Operation="Addition" Value="$(BonusHeroPlanetScience)"  Path="./ClassColonizedStarSystem" TooltipHidden="true"/>
    <Modifier TargetProperty="BonusHeroSystemPrestige"  Operation="Addition" Value="$(BonusHeroPlanetPrestige)" Path="./ClassColonizedStarSystem" TooltipHidden="true"/>

    <Modifier TargetProperty="MaximumPopulation"      Operation="Addition" Value="$(MaximumPopulation)" Path="ClassColonizedPlanet/../ClassStarSystem" TooltipHidden="true"/>

    <BinaryModifier TargetProperty="OverPopulationThreshold"    Operation="Addition"             Left="$(MaximumPopulation)"       BinaryOperation="Multiplication" Right="$(PopulationPercentForOverPopulation)" TooltipHidden="true"/>
    <BinaryModifier TargetProperty="OverPopulationMalus"        Operation="Addition"             Left="$(OverPopulationThreshold)" BinaryOperation="Subtraction"    Right="$(Population)" TooltipHidden="true"/>
    <BinaryModifier TargetProperty="OverPopulationPenalties"    Operation="Addition"             Left="$(OverPopulationMalus)"     BinaryOperation="Multiplication" Right="5" Path="../ClassColonizedStarSystem" TooltipHidden="true"/>

    <Modifier TargetProperty="PlanetHasTemple" Operation="Force" Value="$(HasTemple)" Path="ClassPlanet/ClassPopulationPlanet" TooltipHidden="true"/>

    <!-- For mothership -->
    <BinaryModifier TargetProperty="ShouldOverrideBaseMaximumPopulation" Operation="Addition" Left="$(MaximumPopulationFromMothership)" BinaryOperation="Subtraction"  Right="$(RawMaximumPopulation)"/>
    <Modifier       TargetProperty="BaseMaximumPopulation"               Operation="Addition" Value="$(ShouldOverrideBaseMaximumPopulation)"/>

    <Modifier TargetProperty="Happiness"      Operation="Addition" Value="$(PotentialPlanetRawHappinessIfDeposit)" Path="./ClassColonizedStarSystem" TooltipHidden="true"/>

    <!-- 2023 Patch -->
    <BinaryModifier TargetProperty="PlanetInitialFIDSBonusPerPopulation" Operation="Addition" Left="$(Population)" BinaryOperation="Multiplication"  Right="$(PlanetInitialFIDSBonus)" />

    <!-- Disable maluses if toggle is 0 so that you don't have negative fids -->
    <BinaryModifier TargetProperty="PlanetInitialFIDSBonusPerPopulationFood"     Operation="Addition" Left="$(PlanetInitialFIDSBonusPerPopulation)" BinaryOperation="Multiplication" Right="$(PlanetFoodModifier)"/>
    <BinaryModifier TargetProperty="PlanetInitialFIDSBonusPerPopulationIndustry" Operation="Addition" Left="$(PlanetInitialFIDSBonusPerPopulation)" BinaryOperation="Multiplication" Right="$(PlanetIndustryModifier)"/>
    <BinaryModifier TargetProperty="PlanetInitialFIDSBonusPerPopulationDust"     Operation="Addition" Left="$(PlanetInitialFIDSBonusPerPopulation)" BinaryOperation="Multiplication" Right="$(PlanetDustModifier)"/>
    <BinaryModifier TargetProperty="PlanetInitialFIDSBonusPerPopulationScience"  Operation="Addition" Left="$(PlanetInitialFIDSBonusPerPopulation)" BinaryOperation="Multiplication" Right="$(PlanetScienceModifier)"/>

    <Modifier TargetProperty="PlanetInitialFIDSPenaltyFood"     Operation="Multiplication" Value="$(PlanetFoodModifier)"     Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetInitialFIDSPenaltyIndustry" Operation="Multiplication" Value="$(PlanetIndustryModifier)" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetInitialFIDSPenaltyDust"     Operation="Multiplication" Value="$(PlanetDustModifier)"     Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetInitialFIDSPenaltyScience"  Operation="Multiplication" Value="$(PlanetScienceModifier)"  Path="ClassPlanet"/>

    <!-- Apply fidsi toggles -->
    <Modifier TargetProperty="PlanetFood"     Operation="Multiplication" Value="$(PlanetFoodModifier)"/>
    <Modifier TargetProperty="PlanetIndustry" Operation="Multiplication" Value="$(PlanetIndustryModifier)"/>
    <Modifier TargetProperty="PlanetDust"     Operation="Multiplication" Value="$(PlanetDustModifier)"/>
    <Modifier TargetProperty="PlanetScience"  Operation="Multiplication" Value="$(PlanetScienceModifier)"/>
    <Modifier TargetProperty="PlanetPrestige" Operation="Multiplication" Value="$(PlanetPrestigeModifier)"/>

    <BinaryModifier TargetProperty="PlanetFood"     Operation="Addition" Left="$(PlanetInitialFIDSBonusPerPopulationFood)"     BinaryOperation="Multiplication"  Right="$(PlanetInitialFood)"/>
    <BinaryModifier TargetProperty="PlanetIndustry" Operation="Addition" Left="$(PlanetInitialFIDSBonusPerPopulationIndustry)" BinaryOperation="Multiplication"  Right="$(PlanetInitialIndustry)"/>
    <BinaryModifier TargetProperty="PlanetDust"     Operation="Addition" Left="$(PlanetInitialFIDSBonusPerPopulationDust)"     BinaryOperation="Multiplication"  Right="$(PlanetInitialDust)"/>
    <BinaryModifier TargetProperty="PlanetScience"  Operation="Addition" Left="$(PlanetInitialFIDSBonusPerPopulationScience)"  BinaryOperation="Multiplication"  Right="$(PlanetInitialScience)"/>

    <BinaryModifier TargetProperty="PlanetIndustry" Operation="Addition" Left="$(PlanetIndustryFromSenatorSkill)" BinaryOperation="Multiplication" Right="$(Population)"/>
    <BinaryModifier TargetProperty="PlanetFood"    Operation="Addition" Left="$(PlanetFIDSIFromSenatorSkill)"    BinaryOperation="Multiplication" Right="$(Population)"/>
    <BinaryModifier TargetProperty="PlanetIndustry"    Operation="Addition" Left="$(PlanetFIDSIFromSenatorSkill)"    BinaryOperation="Multiplication" Right="$(Population)"/>
    <BinaryModifier TargetProperty="PlanetScience"    Operation="Addition" Left="$(PlanetFIDSIFromSenatorSkill)"    BinaryOperation="Multiplication" Right="$(Population)"/>
    <BinaryModifier TargetProperty="PlanetDust"    Operation="Addition" Left="$(PlanetFIDSIFromSenatorSkill)"    BinaryOperation="Multiplication" Right="$(Population)"/>
    <BinaryModifier TargetProperty="PlanetPrestige"    Operation="Addition" Left="$(PlanetFIDSIFromSenatorSkill)"    BinaryOperation="Multiplication" Right="$(Population)"/>

    <BinaryModifier TargetProperty="PlanetFood"    Operation="Addition" Left="$(PlanetFIDSFromMothership)"    BinaryOperation="Multiplication" Right="$(Population)"/>
    <BinaryModifier TargetProperty="PlanetIndustry"    Operation="Addition" Left="$(PlanetFIDSFromMothership)"    BinaryOperation="Multiplication" Right="$(Population)"/>
    <BinaryModifier TargetProperty="PlanetScience"    Operation="Addition" Left="$(PlanetFIDSFromMothership)"    BinaryOperation="Multiplication" Right="$(Population)"/>
    <BinaryModifier TargetProperty="PlanetDust"    Operation="Addition" Left="$(PlanetFIDSFromMothership)"    BinaryOperation="Multiplication" Right="$(Population)"/>

    <BinaryModifier TargetProperty="PlanetFood"     Operation="Addition" Left="$(PlanetFoodFromDevelopment)" BinaryOperation="Multiplication" Right="$(Population)"/>
    <BinaryModifier TargetProperty="PlanetIndustry" Operation="Addition" Left="$(PlanetIndustryFromDevelopment)" BinaryOperation="Multiplication" Right="$(Population)"/>
    <BinaryModifier TargetProperty="PlanetDust"     Operation="Addition" Left="$(PlanetDustFromDevelopment)" BinaryOperation="Multiplication" Right="$(Population)"/>
    <BinaryModifier TargetProperty="PlanetScience"  Operation="Addition" Left="$(PlanetScienceFromDevelopment)" BinaryOperation="Multiplication" Right="$(Population)"/>
    <BinaryModifier TargetProperty="PlanetPrestige" Operation="Addition" Left="$(PlanetPrestigeFromDevelopment)" BinaryOperation="Multiplication" Right="$(Population)"/>
    <BinaryModifier TargetProperty="PlanetFIDS"     Operation="Addition" Left="$(PlanetFIDSFromDevelopment)" BinaryOperation="Multiplication" Right="$(Population)"/>
    <BinaryModifier TargetProperty="PlanetFIDSI"    Operation="Addition" Left="$(PlanetFIDSIFromDevelopment)" BinaryOperation="Multiplication" Right="$(Population)"/>

    <!-- ESG -->
    <Modifier TargetProperty="PlanetFood"                         Operation="Percent" Value="$(PlanetFoodDynamic)"           TooltipHidden="true" />
    <Modifier TargetProperty="PlanetIndustry"                     Operation="Percent" Value="$(PlanetIndustryDynamic)"       TooltipHidden="true" />
    <Modifier TargetProperty="PlanetDust"                         Operation="Percent" Value="$(PlanetDustDynamic)"           TooltipHidden="true" />
    <Modifier TargetProperty="PlanetScience"                      Operation="Percent" Value="$(PlanetScienceDynamic)"        TooltipHidden="true" />
    <Modifier TargetProperty="PlanetPrestige"                     Operation="Percent" Value="$(PlanetPrestigeDynamic)"       TooltipHidden="true" />
  </SimulationDescriptor>
</Datatable>