    <Property Name="PlanetIsGasWarm"               BaseValue="0" MaxValue="1"/>
    <Property Name="PlanetIsGasHot"                BaseValue="0" MaxValue="1"/>
    <Property Name="PlanetIsGasBurning"            BaseValue="0" MaxValue="1"/>

    <Property Name="PlanetIsTiny"              BaseValue="0" MaxValue="1"/>
    <Property Name="PlanetIsMedium"            BaseValue="0" MaxValue="1"/>
    <Property Name="PlanetIsLarge"             BaseValue="0" MaxValue="1"/>
    <Property Name="PlanetIsHuge"              BaseValue="0" MaxValue="1"/>

    <!-- Vanilla -->
    <Property Name="RawPopulationFood"          MinValue="Negative"/>
    <Property Name="RawPopulationIndustry"      MinValue="Negative"/>
    <Property Name="RawPopulationDust"          MinValue="Negative"/>
    <Property Name="RawPopulationScience"       MinValue="Negative"/>
    <Property Name="RawPopulationPrestige"      MinValue="Negative"/>

    <Property Name="PopulationFood"             MinValue="Negative"  />
    <Property Name="PopulationIndustry"         MinValue="Negative"  />
    <Property Name="PopulationDust"             MinValue="Negative"  />
    <Property Name="PopulationScience"          MinValue="Negative"  />
    <Property Name="PopulationPrestige"         MinValue="Negative"  />

    <Property Name="DepletingPopulationFood"       MinValue="Negative"  />
    <Property Name="DepletingPopulationIndustry"   MinValue="Negative"  />
    <Property Name="DepletingPopulationDust"       MinValue="Negative"  />
    <Property Name="DepletingPopulationScience"    MinValue="Negative"  />
    <Property Name="DepletingPopulationPrestige"   MinValue="Negative"  />

    <Property Name="ParasitedPopulationFood"       MinValue="Negative"  />
    <Property Name="ParasitedPopulationIndustry"   MinValue="Negative"  />
    <Property Name="ParasitedPopulationDust"       MinValue="Negative"  />
    <Property Name="ParasitedPopulationScience"    MinValue="Negative"  />
    <Property Name="ParasitedPopulationPrestige"   MinValue="Negative"  />

    <Property Name="BonusPopulationFood"									MinValue="Negative"/>

    <Property Name="BonusPopulationIndustry"            MinValue="Negative" />
    <Property Name="BonusPopulationDust"                MinValue="Negative" />
    <Property Name="BonusPopulationScience"             MinValue="Negative" />
    <Property Name="BonusPopulationPrestige"            MinValue="Negative" />
    <Property Name="BonusPopulationTrade"               MinValue="Negative" />
    <Property Name="BonusPopulationDefense"             MinValue="Negative" />
    <Property Name="BonusPopulationHappiness"           MinValue="Negative" />
    <Property Name="BonusPopulationEmpireManpower"      MinValue="Negative" />
    <Property Name="BonusPopulationSystemManpower"      MinValue="Negative" />
    <Property Name="BonusPopulationSystemMaxManpower"   MinValue="Negative" />

    <Property Name="BonusPopulationProcessingPower"                     MinValue="Negative"/>
    <Property Name="BonusPopulationProcessingPowerFromSuperPopulation"  MinValue="Negative"/>

    <Property Name="BonusPopulationFoodHissho"			 MinValue="Negative"/>
    <Property Name="BonusPopulationScienceHissho"        MinValue="Negative" />
    <Property Name="BonusPopulationDustHissho"           MinValue="Negative" />
    <Property Name="BonusPopulationPrestigeHissho"       MinValue="Negative" />
    <Property Name="BonusPopulationSystemManpowerHissho" MinValue="Negative" />
    <Property Name="BonusPopulationDefenseHissho"        MinValue="Negative" />
    <Property Name="HasObedience"                        MinValue="0" MaxValue="1"/>

    <Property Name="BonusPopulationFIDS"           MinValue="Negative" />
    <Property Name="BonusPopulationFIDSPercent"    MinValue="Negative" />
    <Property Name="BonusPopulationFIDSI"          MinValue="Negative" />
    <Property Name="BonusPopulationFIDSIPercent"   MinValue="Negative" />

    <Property Name="BonusPopulationGroundBattleBombardmentAttackerDamages"/>
    <Property Name="BonusPopulationGroundBattleBombardmentAttackerDamagesMin"/>
    <Property Name="BonusPopulationGroundBattleBombardmentAttackerDamagesMax"/>

    <Property Name="PlanetIsMeager"             BaseValue="0" MaxValue="1"/>
    <Property Name="PlanetIsTeeming"            BaseValue="0" MaxValue="1"/>
    <Property Name="PlanetIsTerran"             BaseValue="0" MaxValue="1"/>

    <Property Name="PlanetIsHot"                BaseValue="0" MaxValue="1"/>
    <Property Name="PlanetIsTemperate"          BaseValue="0" MaxValue="1"/>
    <Property Name="PlanetIsCold"               BaseValue="0" MaxValue="1"/>

    <Property Name="PlanetIsGas"                BaseValue="0" MaxValue="1"/>
    <Property Name="PlanetIsFake"               BaseValue="0" MaxValue="1"/>
    <Property Name="PlanetIsSmall"              BaseValue="0" MaxValue="1"/>

    <Property Name="PlanetHasAnomaly"           BaseValue="0" MaxValue="1"/>
    <Property Name="PlanetHasStrategicDeposit"  BaseValue="0" MaxValue="1"/>
    <Property Name="PlanetHasLuxuryDeposit"     BaseValue="0" MaxValue="1"/>

     <Property Name="HappinessStarSystemIsMutinous" BaseValue="0"/>
     <Property Name="HappinessStarSystemIsUnhappy" BaseValue="0"/>
    <Property Name="HappinessStarSystemIsHappy" BaseValue="0"/>
     <Property Name="HappinessStarSystemIsEcstatic" BaseValue="0"/>

    <Property Name="RepletionPerTurn" BaseValue="0"/>

    <Property Name="PlanetHasTemple" BaseValue="0" MaxValue="1"/>
	
	<Property Name="PopulationIsBoosted" BaseValue="0" MaxValue="1"/>
	<Property Name="PopulationAuxValue0" BaseValue="0"/>
	<Property Name="PopulationAuxValue1" BaseValue="0"/>
	<Property Name="PopulationAuxValue2" BaseValue="0"/>

    <!-- #### ECONOMY PROCESS ##### -->
    <!-- Common trunk -->
    <BinaryModifier   TargetProperty="BonusPopulationFood"        Operation="Addition"     Left="$(BonusPopulationFoodHissho)"     BinaryOperation="Multiplication" Right="$(HasObedience)"   />
    <BinaryModifier   TargetProperty="BonusPopulationScience"     Operation="Addition"     Left="$(BonusPopulationScienceHissho)"  BinaryOperation="Multiplication" Right="$(HasObedience)"   />
    <BinaryModifier   TargetProperty="BonusPopulationPrestige"    Operation="Addition"     Left="$(BonusPopulationPrestigeHissho)" BinaryOperation="Multiplication" Right="$(HasObedience)"   />
    <BinaryModifier   TargetProperty="BonusPopulationDust"        Operation="Addition"     Left="$(BonusPopulationDustHissho)"     BinaryOperation="Multiplication" Right="$(HasObedience)"  />

    <Modifier   TargetProperty="BonusPopulationFood"              Operation="Addition"     Value="$(BonusPopulationFIDSI)"   />
    <Modifier   TargetProperty="BonusPopulationIndustry"          Operation="Addition"     Value="$(BonusPopulationFIDSI)"   />
    <Modifier   TargetProperty="BonusPopulationDust"              Operation="Addition"     Value="$(BonusPopulationFIDSI)"   />
    <Modifier   TargetProperty="BonusPopulationScience"           Operation="Addition"     Value="$(BonusPopulationFIDSI)"   />
    <Modifier   TargetProperty="BonusPopulationPrestige"          Operation="Addition"     Value="$(BonusPopulationFIDSI)"   />

    <Modifier   TargetProperty="BonusPopulationFood"              Operation="Addition"     Value="$(BonusPopulationFIDS)"   />
    <Modifier   TargetProperty="BonusPopulationIndustry"          Operation="Addition"     Value="$(BonusPopulationFIDS)"   />
    <Modifier   TargetProperty="BonusPopulationDust"              Operation="Addition"     Value="$(BonusPopulationFIDS)"   />
    <Modifier   TargetProperty="BonusPopulationScience"           Operation="Addition"     Value="$(BonusPopulationFIDS)"   />

    <Modifier   TargetProperty="BonusPopulationProcessingPower"             Operation="Addition"     Value="$(BonusPopulationProcessingPowerFromSuperPopulation)"   />
    <BinaryModifier   TargetProperty="MaximumEmpireProcessingPowerStock"    Operation="Addition" Left="$(BonusPopulationProcessingPower)" BinaryOperation="Multiplication" Right="$(PopulationCount)" Path="../ClassEmpire"/>

    <!-- ESG Changes -->
    <Modifier TargetProperty="BonusPopulationFood"      Operation="Multiplication" Value="$(PopulationPercentModifier)"/>
    <Modifier TargetProperty="BonusPopulationIndustry"  Operation="Multiplication" Value="$(PopulationPercentModifier)"/>
    <Modifier TargetProperty="BonusPopulationDust"      Operation="Multiplication" Value="$(PopulationPercentModifier)"/>
    <Modifier TargetProperty="BonusPopulationScience"   Operation="Multiplication" Value="$(PopulationPercentModifier)"/>
    <Modifier TargetProperty="BonusPopulationHappiness" Operation="Multiplication" Value="$(PopulationPercentModifier)"/>

    <Modifier   TargetProperty="PopulationFood"         Operation="Addition"    Value="$(RawPopulationFood)"       Priority="-3" />
    <Modifier   TargetProperty="PopulationIndustry"     Operation="Addition"    Value="$(RawPopulationIndustry)"   Priority="-3" />
    <Modifier   TargetProperty="PopulationDust"         Operation="Addition"    Value="$(RawPopulationDust)"       Priority="-3" />
    <Modifier   TargetProperty="PopulationScience"      Operation="Addition"    Value="$(RawPopulationScience)"    Priority="-3" />
    <Modifier   TargetProperty="PopulationPrestige"     Operation="Addition"    Value="$(RawPopulationPrestige)"   Priority="-3" />

    <!-- Special trunk -->
    <Modifier   TargetProperty="PopulationFood"         Operation="Addition"     Value="$(BonusPopulationFood)"         Priority="-2" />
    <Modifier   TargetProperty="PopulationIndustry"     Operation="Addition"     Value="$(BonusPopulationIndustry)"     Priority="-2" />
    <Modifier   TargetProperty="PopulationDust"         Operation="Addition"     Value="$(BonusPopulationDust)"         Priority="-2" />
    <Modifier   TargetProperty="PopulationScience"      Operation="Addition"     Value="$(BonusPopulationScience)"      Priority="-2" />
    <Modifier   TargetProperty="PopulationPrestige"     Operation="Addition"     Value="$(BonusPopulationPrestige)"     Priority="-2" />

    <Modifier   TargetProperty="PopulationFood"         Operation="Percent"     Value="$(BonusPopulationFIDSPercent)"   Priority="-2" />
    <Modifier   TargetProperty="PopulationIndustry"     Operation="Percent"     Value="$(BonusPopulationFIDSPercent)"   Priority="-2" />
    <Modifier   TargetProperty="PopulationDust"         Operation="Percent"     Value="$(BonusPopulationFIDSPercent)"   Priority="-2" />
    <Modifier   TargetProperty="PopulationScience"      Operation="Percent"     Value="$(BonusPopulationFIDSPercent)"   Priority="-2" />

    <Modifier   TargetProperty="PopulationFood"         Operation="Percent"     Value="$(BonusPopulationFIDSIPercent)"  Priority="-2" />
    <Modifier   TargetProperty="PopulationIndustry"     Operation="Percent"     Value="$(BonusPopulationFIDSIPercent)"  Priority="-2" />
    <Modifier   TargetProperty="PopulationDust"         Operation="Percent"     Value="$(BonusPopulationFIDSIPercent)"  Priority="-2" />
    <Modifier   TargetProperty="PopulationScience"      Operation="Percent"     Value="$(BonusPopulationFIDSIPercent)"  Priority="-2" />
    <Modifier   TargetProperty="PopulationPrestige"     Operation="Percent"     Value="$(BonusPopulationFIDSIPercent)"  Priority="-2" />

    <!-- Common trunk -->
    <Modifier   TargetProperty="PopulationIndustry"     Operation="Subtraction" Value="$(RawPopulationIndustry)"   Priority="1" />
    <Modifier   TargetProperty="PopulationDust"         Operation="Subtraction" Value="$(RawPopulationDust)"       Priority="-1" />
    <Modifier   TargetProperty="PopulationScience"      Operation="Subtraction" Value="$(RawPopulationScience)"    Priority="-1" />
    <Modifier   TargetProperty="PopulationPrestige"     Operation="Subtraction" Value="$(RawPopulationPrestige)"   Priority="-1" />
    <Modifier   TargetProperty="PopulationFood"         Operation="Subtraction" Value="$(RawPopulationFood)"       Priority="-1" />

    <BinaryModifier   TargetProperty="DepletingPopulationFood"     Operation="Subtraction"    Left="$(RawPopulationFood)"      BinaryOperation="Division" Right="$(PlanetDepletionFIDSRemaining)"    Priority="-1" />
    <BinaryModifier   TargetProperty="DepletingPopulationIndustry" Operation="Subtraction"    Left="$(RawPopulationIndustry)"  BinaryOperation="Division" Right="$(PlanetDepletionFIDSRemaining)"    Priority="-1" />
    <BinaryModifier   TargetProperty="DepletingPopulationDust"     Operation="Subtraction"    Left="$(RawPopulationDust)"      BinaryOperation="Division" Right="$(PlanetDepletionFIDSRemaining)"    Priority="-1" />
    <BinaryModifier   TargetProperty="DepletingPopulationScience"  Operation="Subtraction"    Left="$(RawPopulationScience)"   BinaryOperation="Division" Right="$(PlanetDepletionFIDSRemaining)"    Priority="-1" />
    <BinaryModifier   TargetProperty="DepletingPopulationPrestige" Operation="Subtraction"    Left="$(RawPopulationPrestige)"  BinaryOperation="Division" Right="$(PlanetDepletionFIDSRemaining)"    Priority="-1" />

    <BinaryModifier   TargetProperty="ParasitedPopulationFood"          Operation="Subtraction"    Left="$(RawPopulationFood)"     BinaryOperation="Division" Right="$(PlanetDepletionFIDSRemaining)"  Priority="-1" />
    <BinaryModifier   TargetProperty="ParasitedPopulationIndustry"      Operation="Subtraction"    Left="$(RawPopulationIndustry)" BinaryOperation="Division" Right="$(PlanetDepletionFIDSRemaining)"  Priority="-1" />
    <BinaryModifier   TargetProperty="ParasitedPopulationDust"          Operation="Subtraction"    Left="$(RawPopulationDust)"     BinaryOperation="Division" Right="$(PlanetDepletionFIDSRemaining)"  Priority="-1" />
    <BinaryModifier   TargetProperty="ParasitedPopulationScience"       Operation="Subtraction"    Left="$(RawPopulationScience)"  BinaryOperation="Division" Right="$(PlanetDepletionFIDSRemaining)"  Priority="-1" />
    <BinaryModifier   TargetProperty="ParasitedPopulationPrestige"      Operation="Subtraction"    Left="$(RawPopulationPrestige)" BinaryOperation="Division" Right="$(PlanetDepletionFIDSRemaining)"  Priority="-1" />

    <!-- Special trunk -->
    <BinaryModifier       TargetProperty="IndustryFromPopulation"   Operation="Addition"    Left="$(PopulationIndustry)" BinaryOperation="Multiplication" Right="$(PopulationCount)"    Path="./ClassColonizedPlanet"         />
    <BinaryModifier       TargetProperty="DustFromPopulation"       Operation="Addition"    Left="$(PopulationDust)"     BinaryOperation="Multiplication" Right="$(PopulationCount)"    Path="./ClassColonizedPlanet"         />
    <BinaryModifier       TargetProperty="ScienceFromPopulation"    Operation="Addition"    Left="$(PopulationScience)"  BinaryOperation="Multiplication" Right="$(PopulationCount)"    Path="./ClassColonizedPlanet"         />
    <BinaryModifier       TargetProperty="PrestigeFromPopulation"   Operation="Addition"    Left="$(PopulationPrestige)" BinaryOperation="Multiplication" Right="$(PopulationCount)"    Path="./ClassColonizedPlanet"         />
    <BinaryModifier       TargetProperty="FoodFromPopulation"       Operation="Addition"    Left="$(PopulationFood)"     BinaryOperation="Multiplication" Right="$(PopulationCount)"    Path="./ClassColonizedPlanet"         />

    <!-- #### END ECONOMY PROCESS ##### -->

    <BinaryModifier       TargetProperty="PlanetDepletionPerTurn"               Operation="Subtraction" Left="$(RepletionPerTurn)"              BinaryOperation="Multiplication"        Right="$(PopulationCount)"      Path="./ClassPlanet"   TooltipHidden="true" />

    <BinaryModifier   TargetProperty="Happiness"                Operation="Addition"     Left="$(BonusPopulationHappiness)"      BinaryOperation="Multiplication" Right="$(PopulationCount)"     Priority="-2" Path="../ClassStarSystem" />
    <BinaryModifier   TargetProperty="BonusPopulationDefense"   Operation="Addition"     Left="$(BonusPopulationDefenseHissho)"  BinaryOperation="Multiplication" Right="$(HasObedience)"        Priority="-2" />
    <BinaryModifier   TargetProperty="MaximumSystemManpower"    Operation="Addition"     Left="$(BonusPopulationDefense)"        BinaryOperation="Multiplication" Right="$(PopulationCount)"     Priority="-2" Path="../ClassStarSystem" />
    <BinaryModifier   TargetProperty="NetSystemManpower"        Operation="Addition"     Left="$(BonusPopulationSystemManpower)" BinaryOperation="Multiplication" Right="$(PopulationCount)"     Priority="-2" Path="../ClassStarSystem" />
    <BinaryModifier   TargetProperty="EmpireManpower"           Operation="Addition"     Left="$(BonusPopulationEmpireManpower)" BinaryOperation="Multiplication" Right="$(PopulationCount)"     Priority="-2" Path="../ClassEmpire" />

    <BinaryModifier		TargetProperty="GroundBattleBombardmentAttackerDamagesMin"	Operation="Addition" Left="$(BonusPopulationGroundBattleBombardmentAttackerDamagesMin)" BinaryOperation="Multiplication" Right="$(PopulationCount)"  Priority="-2" Path="../ClassColonizedStarSystem" />
    <BinaryModifier		TargetProperty="GroundBattleBombardmentAttackerDamagesMax"	Operation="Addition" Left="$(BonusPopulationGroundBattleBombardmentAttackerDamagesMax)" BinaryOperation="Multiplication" Right="$(PopulationCount)"  Priority="-2" Path="../ClassColonizedStarSystem" />

    <BinaryModifier		TargetProperty="GroundBattleBombardmentAttackerDamagesMin"	Operation="Addition" Left="$(BonusPopulationGroundBattleBombardmentAttackerDamages)" BinaryOperation="Multiplication" Right="$(PopulationCount)"  Priority="-2" Path="../ClassColonizedStarSystem" />
    <BinaryModifier		TargetProperty="GroundBattleBombardmentAttackerDamagesMax"	Operation="Addition" Left="$(BonusPopulationGroundBattleBombardmentAttackerDamages)" BinaryOperation="Multiplication" Right="$(PopulationCount)"  Priority="-2" Path="../ClassColonizedStarSystem" />

    <Modifier         TargetProperty="PopulationTypeCount"     Operation="Addition" Value="1"                  Path="../ClassPlanet"            TooltipHidden="true"/>
    <Modifier         TargetProperty="Population"              Operation="Addition" Value="$(PopulationCount)" Path="../ClassPlanet"            TooltipHidden="true"/>
  </SimulationDescriptor>

  <SimulationDescriptor Name="ClassPopulationStarSystem" Type="ClassPopulation">
    <Property Name="PopulationTypeCount"          BaseValue="1"/>
    <Property Name="PopulationCountSuperiorToZero"			  BaseValue="0" MinValue="0" MaxValue="1"/>

    <Property Name="PopulationScoreStock"         BaseValue="0"                 MinValue="Negative"     IsSealed="true"    IsSerializable="true"/>
    <Property Name="NetPopulationScore"           BaseValue="0"                 MinValue="-100"    />

    <Property Name="PoliticsTrend01"              BaseValue="0" IsSealed="true"/>
    <Property Name="PoliticsTrend02"              BaseValue="0" IsSealed="true"/>
    <Property Name="PoliticsTrend03"              BaseValue="0" IsSealed="true"/>
    <Property Name="PoliticsTrend04"              BaseValue="0" IsSealed="true"/>
    <Property Name="PoliticsTrend05"              BaseValue="0" IsSealed="true"/>
    <Property Name="PoliticsTrend06"              BaseValue="0" IsSealed="true"/>

    <!-- MinValue is a safe guard. -->
    <Property Name="PoliticsScoreDecayStep"       BaseValue="1"  MinValue="0.1"/>

    <Property Name="PopulationGrowthMalus"        BaseValue="1" MinValue="Negative"/>
    <Property Name="NetPopulationScoreModifier"   BaseValue="1" MinValue="Negative"/>

    <Property Name="CannotMigrate"               MinValue="0" MaxValue="1" />

    <Property Name="ShouldPopulationGrow"        BaseValue="1" MinValue="-1" MaxValue="1"/>
    <Property Name="ShouldPopulationIncrease"    BaseValue="1" MinValue="-1" MaxValue="1"/>

    <!--Bonus Collection Properties-->
    <Property Name="SystemFIDSIPercentBonusFromCollection"/>
    <Property Name="SystemGrowthBonusFromCollection"/>
    <Property Name="SystemProductionBonusFromCollection"/>
    <Property Name="SystemMoneyBonusFromCollection"/>
    <Property Name="SystemResearchBonusFromCollection"/>
    <Property Name="SystemResearchZvaliBonusFromCollection01"/>
    <Property Name="SystemResearchZvaliBonusFromCollection02"/>
    <Property Name="SystemEmpirePointBonusFromCollection"/>
    <Property Name="HappinessBonusFromCollection"/>
    <Property Name="ShipsConstructionProductionBonusFromCollection"/>
    <Property Name="BuildingConstructionBonusFromCollection"/>
    <Property Name="BuildingBuyoutReductionBonusFromCollection"/>
    <Property Name="MaximumSystemManpowerBonusFromCollection"/>
    <Property Name="FlatMaximumSystemManpowerBonusFromCollection"/>
    <Property Name="NetSystemManpowerBonusFromCollection"/>
    <Property Name="NetSystemManpowerPercentBonusFromCollection" BaseValue="0"/>
    <Property Name="NetSystemManpowerBonusFromCollectionHissho"/>
    <Property Name="GroundBattleBombardmentAttackerDamagesBonusFromCollection"/>
    <Property Name="LuxuryBonusFromCollection"/>
    <Property Name="StrategicBonusFromCollection"/>
    <Property Name="VisionRangeBonusFromCollection"/>

    <Property Name="SystemGrowthBonusFromCollectionHissho"/>
    <Property Name="DockedFleetsHealthRegenerationRatioBonusFromCollection"/>

    <BinaryModifier TargetProperty="PopulationType"	Operation="Addition"   Left="1" BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassStarSystem" TooltipHidden="true"/>
    <Modifier TargetProperty="PopulationCountSuperiorToZero" Operation="Addition"   Value="$(PopulationCount)"      TooltipHidden="true"/>

    <!-- Score evolution -->
    <BinaryModifier TargetProperty="NetPopulationScore" Operation="Addition" Left="50"  BinaryOperation="Multiplication" Right="$(NetPopulationScoreModifier)" TooltipHidden="true"/>
    <Modifier TargetProperty="NetPopulationScore"          Operation="Multiplication"                	Value="$(PopulationGrowthMalus)"           		TooltipHidden="true"/>
    <!--<Modifier TargetProperty="NetPopulationScore"          Operation="Multiplication"                	Value="$(ShouldPopulationGrow)"                	TooltipHidden="true"   Priority="2"/>-->

    <!--Bonus Collection checks if population is > 0-->
    <BinaryModifier TargetProperty="SystemGrowthFromCollection"				Operation="Addition"    Left="$(SystemFIDSIPercentBonusFromCollection)"		BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassColonizedStarSystem"/>
    <BinaryModifier TargetProperty="SystemProductionFromCollection"			Operation="Addition"    Left="$(SystemFIDSIPercentBonusFromCollection)"		BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassColonizedStarSystem"/>
    <BinaryModifier TargetProperty="SystemMoneyFromCollection"				Operation="Addition"    Left="$(SystemFIDSIPercentBonusFromCollection)"		BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassColonizedStarSystem"/>
    <BinaryModifier TargetProperty="SystemResearchFromCollection"			Operation="Addition"    Left="$(SystemFIDSIPercentBonusFromCollection)"		BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassColonizedStarSystem"/>
    <BinaryModifier TargetProperty="SystemEmpirePointFromCollection"		Operation="Addition"    Left="$(SystemFIDSIPercentBonusFromCollection)"		BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassColonizedStarSystem"/>
    <BinaryModifier TargetProperty="SystemGrowthFromCollection"				Operation="Addition"    Left="$(SystemGrowthBonusFromCollection)"			BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassColonizedStarSystem"/>
    <BinaryModifier TargetProperty="SystemProductionFromCollection"			Operation="Addition"    Left="$(SystemProductionBonusFromCollection)"		BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassColonizedStarSystem" />
    <BinaryModifier TargetProperty="SystemMoneyFromCollection"				Operation="Addition"    Left="$(SystemMoneyBonusFromCollection)"			BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassColonizedStarSystem"/>
    <BinaryModifier TargetProperty="SystemResearchFromCollection"			Operation="Addition"	Left="$(SystemResearchBonusFromCollection)"			BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassColonizedStarSystem"/>
    <BinaryModifier TargetProperty="SystemResearchFromCollection"			Operation="Addition"    Left="$(SystemResearchZvaliBonusFromCollection01)"	BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassColonizedStarSystem,HappinessStatusStarSystem3"/>
    <BinaryModifier TargetProperty="SystemResearchFromCollection"			Operation="Addition"	Left="$(SystemResearchZvaliBonusFromCollection02)"	BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassColonizedStarSystem,HappinessStatusStarSystem4"/>
    <BinaryModifier TargetProperty="SystemEmpirePointFromCollection"		Operation="Addition"    Left="$(SystemEmpirePointBonusFromCollection)"		BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassStarSystem" />
    <BinaryModifier TargetProperty="HappinessFromCollection"				Operation="Addition"    Left="$(HappinessBonusFromCollection)"				BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassStarSystem" />

    <BinaryModifier TargetProperty="SystemGrowthFromCollection"				Operation="Addition"    Left="$(SystemGrowthBonusFromCollectionHissho)"		BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassColonizedStarSystem,HasObedience"/>
    <BinaryModifier TargetProperty="DockedFleetsHealthRegenerationRatioFromCollection" Operation="Addition" Left="$(DockedFleetsHealthRegenerationRatioBonusFromCollection)" BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassStarSystem" />

    <BinaryModifier TargetProperty="NetSystemManpower"                      Operation="Addition"    Left="$(NetSystemManpowerBonusFromCollectionHissho)"                BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassStarSystem,HasObedience" />
    <BinaryModifier TargetProperty="ShipProductionCostReduction"			Operation="Subtraction" Left="$(ShipsConstructionProductionBonusFromCollection)"            BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassStarSystem"/>
    <BinaryModifier TargetProperty="BuildingProductionCostReduction"		Operation="Subtraction" Left="$(BuildingConstructionBonusFromCollection)"                   BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassStarSystem" />
    <BinaryModifier TargetProperty="BuildingBuyoutReduction"				Operation="Addition"    Left="$(BuildingBuyoutReductionBonusFromCollection)"                BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassColonizedStarSystem"/>
