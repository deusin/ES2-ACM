    <Property Name="PlanetIsSnow"                  BaseValue="0" MaxValue="1"/>
    <Property Name="PlanetIsArid"                  BaseValue="0" MaxValue="1"/>
    <Property Name="PlanetIsDesert"                BaseValue="0" MaxValue="1"/>
    <Property Name="PlanetIsAsh"                   BaseValue="0" MaxValue="1"/>
    <Property Name="PlanetIsIce"                   BaseValue="0" MaxValue="1"/>
    <Property Name="PlanetIsSwamp"                 BaseValue="0" MaxValue="1"/>
    <Property Name="PlanetIsBarren"                BaseValue="0" MaxValue="1"/>
