    <Property Name="DepositValue" BaseValue="2"/>
    <Property Name="ResourceBonus"/>
    <Modifier TargetProperty="DepositValue" Operation="Addition" Value="$(ResourceBonus)" Priority="5"/>
    </SimulationDescriptor>
  <SimulationDescriptor Name="ClassColonizedResourceDeposit" Type="ClassColonizedResourceDeposit"> </SimulationDescriptor>

  <!--  Deposit Size  -->
